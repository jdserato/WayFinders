<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="401.0" prefWidth="690.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Management_Controller">
   <children>
      <VBox fx:id="vbMainPane" alignment="CENTER" minHeight="0.0" minWidth="0.0" prefHeight="0.0" prefWidth="640.0" spacing="45.0">
         <children>
            <HBox alignment="CENTER" spacing="75.0">
               <children>
                  <ImageView fx:id="ivAddBus" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#handleAddBusClicked" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@res/add_bus.png" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="150.0" fitWidth="200.0" onMouseClicked="#handleAddBusClicked" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@res/edit_bus.png" />
                     </image>
                  </ImageView>
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="75.0">
               <children>
                  <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@res/edit_fares.png" />
                     </image></ImageView>
                  <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@res/delete_bus.png" />
                     </image></ImageView>
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox fx:id="vbEditBuses" alignment="CENTER" minHeight="0.0" minWidth="0.0" prefHeight="0.0" visible="false">
         <children>
            <HBox fx:id="hbEditBus" prefHeight="25.0" prefWidth="631.0" spacing="25.0">
               <children>
                  <ComboBox fx:id="cbBus" prefHeight="25.0" prefWidth="148.0" promptText="Bus Company" />
                  <ComboBox fx:id="cbDestination" disable="true" prefHeight="25.0" prefWidth="148.0" promptText="Destination" />
                  <ComboBox fx:id="cbType" disable="true" prefHeight="25.0" prefWidth="159.0" promptText="Type" />
                  <Button fx:id="btnSearch" defaultButton="true" disable="true" mnemonicParsing="false" onMouseClicked="#handleEditClicked" prefHeight="25.0" prefWidth="88.0" text="Edit" />
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </HBox>
            <HBox fx:id="hbDetail" alignment="CENTER" spacing="15.0" visible="false">
               <children>
                  <VBox alignment="CENTER_RIGHT" prefHeight="200.0" prefWidth="100.0" spacing="8.0">
                     <children>
                        <Label text="Bus Company" />
                        <Label text="Type" />
                        <Label text="Destination" />
                        <Label text="Wing Area" />
                        <Label text="Bay Number" />
                        <Label text="First Departure" />
                        <Label text="Last Departure" />
                        <Label text="Number of Trips" />
                        <Label text="Number of Buses" />
                        <Label text="Maximum Fare" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <TextField fx:id="tfCompany" editable="false" />
                        <TextField fx:id="tfType" />
                        <TextField fx:id="tfDestination" />
                        <TextField fx:id="tfWingArea" />
                        <TextField fx:id="tfBayNumber" />
                        <TextField fx:id="tfFirstDep" />
                        <TextField fx:id="tfLastDep" />
                        <TextField fx:id="tfTrips" />
                        <TextField fx:id="tfBuses" />
                        <TextField fx:id="tfFare" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <VBox alignment="CENTER">
               <children>
                  <Button fx:id="btnSave" mnemonicParsing="false" onMouseClicked="#handleSaveClicked" prefHeight="25.0" prefWidth="126.0" text="Save" visible="false" />
                  <Button fx:id="btnAdd" mnemonicParsing="false" onMouseClicked="#handleAddClicked" prefHeight="25.0" prefWidth="126.0" text="Add" visible="false" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
   </padding>
</VBox>
